	<% @normalclaims.each do |c|%>
	<%status = c.status%>
	<%if c.remarks.nil?%>
	<tr>
	<%else%>
	<tr class = 'warning'>
	<%end%>
		<td><%=c.id%></td>
		<td><%= c.userid%></td>
		<td><%= c.clubid%></td>
		<td><%= c.amount%></td>
		<td><%= c.category%></td>
		<td><%= c.event%></td>
		<td><%= c.created_at%></td>
		<td><%= c.updated_at%></td>
		<td><%= c.status%></td>
		<td><a href="<%= c.supportdoc.url%>" class="btn btn-sm btn-primary">Download</td>
