		<td><a href="/claims/editclaim?id=<%=c.id%>" class="btn btn-sm btn-primary">Edit</td>
		<%if c.remarks.nil?%>
		<td><a href="/claims/submitclaim?id=<%=c.id%>" class="btn btn-sm btn-primary">Submit</td>
		<%else%>
		<td><a href="/claims/resubmitclaim?id=<%=c.id%>" class="btn btn-sm btn-primary">ReSubmit</td>
		<td><a href="#", onclick="viewremark('<%=c.remarks%>')" class="btn btn-sm btn-primary">View Remarks</a></td>
		<%end%>
		<%end%>
	</tr>
	<%end%>

</table>

