<%if session[:role].include? 'smusafinsec'%>

<h1>Listing clubusers</h1>

<table>
  <thead>
    <tr>
      <th>Userid</th>
      <th>Clubid</th>
      <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clubusers.each do |clubuser| %>
      <tr>
        <td><%= clubuser.userid %></td>
        <td><%= clubuser.clubid %></td>
        <td><%= clubuser.role %></td>
        <td><%= link_to 'Show', clubuser %></td>
        <td><%= link_to 'Edit', edit_clubuser_path(clubuser) %></td>
        <td><%= link_to 'Destroy', clubuser, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Clubuser', '/clubusers/new2' %>

<%end%>
