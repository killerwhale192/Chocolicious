<div class='row'>
	<div class='col-sm-12'>
		<div class='page-header'>
			<h1 class='pull-left'><i class='icon-file-alt'></i><span> Batch Reconciliation</span></h1>
		</div>
	</div>
</div>
<%if session[:role].include? 'smusafinsec'%>
<div class='row'>
	<div class='col-sm-12 col-lg-12'>
		<div class='box bordered-box'>
			<div class='box-content box-double-padding'>
				<%= form_tag '/chief/autoreconcile', :multipart => true do %>
				<fieldset>
					<div class='col-sm-4'>
						<div class='lead'>
							<i class='icon-file-alt text-contrast'></i>
							Notes On Uploading Report
						</div>
						<small class='text-muted'>Upload Club Report received for the AIS to reconcile the submitted claims. </small>
					</div>
					<div class='col-sm-7 col-sm-offset-1'>
						<div class='form-group'>
							<label>Attach Report*</label>
								<%= file_field_tag :file, :required=> true %>
							</div>
				</fieldset>				
				<div class='form-actions form-actions-padding' style='margin-bottom: 0;'>
					<div class='text-right'>
						<%= submit_tag "Upload", :class=>"btn btn-primary btn-lg blue-background" %>
					</div>
				</div>
				<%end%>
			</div>
		</div>
	</div>
</div>
<%end%>

