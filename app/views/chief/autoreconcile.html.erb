<div class='row'>
	<div class='col-sm-12'>
		<div class='page-header'>
			<h1 class='pull-left'><i class='icon-file-alt'></i><span> Batch Reconciliation</span></h1>
		</div>
	</div>
</div>
<div class='row'>
	<div class="col-sm-12">
		<div class="box bordered-box blue-border">
			<div class="box-header blue-background">
				<div class="title">
					Reconciled Items
				</div>
				<div class="actions">
					<a class="btn box-collapse btn-xs btn-link" href="#"><i></i> </a>
				</div>
			</div>
			<div class="box-content box-no-padding">
				<div class="responsive-table">
					<div class="scrollable-area">
						<table class="data-table table table-striped" style="margin-bottom:0;">
							<thead>
								<tr>
									<th>Fund</th>
									<th>Post date</th>
									<th>Doc Num</th>
									<th>Item Text</th>
									<th>GL Balance</th>
									<th>Expenses</th>
									<th>Revenue</th>
								</tr>
							</thead>
							<tbody>
								<% @superarray.each do |s|%>
								<tr>
									<% for i in 0..s.size-1%>
									<td><%= s[i]%></td>
									<%end%>
								</tr>
								<%end%>
							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>